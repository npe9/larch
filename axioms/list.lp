% Lists

set name list

declare sorts Element, List
declare variables e, e1: Element, x, y, z: List
declare operators
  null     :               -> List
  __ |- __ : Element, List -> List
  __ || __ : List, List    -> List
  rev      : List          -> List
  ..

assert 
  sort List generated by null, |-;
  null ||  x = x;
  (e |- y) || z = e |- (y || z);
  rev(null) = null;
  rev(e |- y) = rev(y) || (e |- null);
  e |- x ~= null;
  e |- x = e1 |- y <=> (e = e1 /\ x = y)
  ..
